                                <ComboBox  VerticalAlignment="Center" IsEnabled="{Binding GenericBool}"
SelectedItem="{Binding Path=Status,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                                      DisplayMemberPath="StatusName"
