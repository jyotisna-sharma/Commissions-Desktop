            <TextBox VerticalAlignment="Center" Grid.Column="6" IsReadOnly="True" Grid.Row="2" Text="{Binding SelectedFollowupIssueDetail.Enroll_Eligible}"></TextBox>
            <TextBox VerticalAlignment="Center" Grid.Column="6" IsReadOnly="True" Grid.Row="3" Text="{Binding SelectedFollowupIssueDetail.Mode}"></TextBox>
            <TextBox VerticalAlignment="Center" Grid.Column="6" IsReadOnly="True" Grid.Row="4" Text="{Binding SelectedFollowupIssueDetail.ModalPremium,StringFormat=c}"></TextBox>
            <TextBox  VerticalAlignment="Center" Grid.Column="8" IsReadOnly="True" Text="{Binding SelectedFollowupIssueDetail.LastUser}"></TextBox>
            <TextBox VerticalAlignment="Center" Grid.Column="8" IsReadOnly="True" Grid.Row="1" Text="{Binding SelectedFollowupIssueDetail.LastUpdate,StringFormat=MM/dd/yyyy,Converter={StaticResource stringtodateConv}}"></TextBox>
            <TextBox VerticalAlignment="Center" Grid.Column="8" IsReadOnly="True" Grid.Row="2" Text="{Binding SelectedFollowupIssueDetail.TrackingNumber}"></TextBox>
            <TextBox VerticalAlignment="Center" Grid.Column="8" IsReadOnly="True" Grid.Row="3" Text="{Binding SelectedFollowupIssueDetail.Created,Converter={StaticResource stringtodateConv},StringFormat=MM/dd/yyyy}"></TextBox>
